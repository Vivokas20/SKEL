schema df_manufacturer(manufacturer_id:int, open_year:str, name:str, num_of_factories:int, num_of_shops:int);

table df_manufacturer(df_manufacturer);

query q1
`select name,
       manufacturer_id
from df_manufacturer df_manufacturer`;

query q2
