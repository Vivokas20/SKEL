schema df_i002(ID:int, NAME:str, EMAIL:str);

table df_i002(df_i002);

query q1
`select name
from
  (select email, name, count(*) as n
   from input1
   group by email, name) t
where n > 1.0`;

query q2
`select name
from
  (select name,
          count(*) as n
   from input0
   group by name)
where (n > 1.0
       and n >= 1.0)`;

verify q1 q2;