schema df_player(player_id:int, player:str, years_played:str, total_wl:str, singles_wl:str, doubles_wl:str, team:int);

table df_player(df_player);

query q1
`select df_player,
       years_played
from df_player df_player`;

query q2
`select distinct player,
                years_played
from input0`;

verify q1 q2;