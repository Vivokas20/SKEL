schema df_member(member_id:int, name:str, country:str, college_id:int);

table df_member(df_member);

query q1
`select country
from df_member df_member
group by country
having count(*) > 2`;

query q2
`select country
from
  (select country,
          count(*) as n
   from input0
   group by country)
where (n <= 2.0
       or n > 2.0)`;

verify q1 q2;