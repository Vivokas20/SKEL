schema df_medicine(id:int, name:str, trade_name:str, fda_approved:str);

table df_medicine(df_medicine);

query q1
`select name,
       trade_name
from df_medicine df_medicine
where fda_approved = 'yes'`;

query q2
`select distinct name,
                trade_name
from input0
where (fda_approved = 'yes')`;

verify q1 q2;