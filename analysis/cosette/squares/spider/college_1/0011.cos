schema df_course(crs_code:str, dept_code:str, crs_description:str, crs_credit:str);

table df_course(df_course);

query q1
`select crs_credit,
       crs_description
from df_course df_course
where crs_code = 'qm-261'`;

query q2
`select distinct crs_credit,
                crs_description
from input0
where (crs_code = 'qm-261')`;

verify q1 q2;