schema df_driver(driver_id:int, name:str, party:str, home_city:str, age:int);

table df_driver(df_driver);

query q1
`select name
from df_driver df_driver
where home_city = 'hartford'
  and age < 40`;

query q2
`select name
from df_driver df_driver
where (home_city = 'hartford'
       and age < 40.0)`;

verify q1 q2;