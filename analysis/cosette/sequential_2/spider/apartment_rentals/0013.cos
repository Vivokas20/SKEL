schema df_guests(guest_id:int, gender_code:str, guest_first_name:str, guest_last_name:str, date_of_birth:str);

table df_guests(df_guests);

query q1
`select date_of_birth
from df_guests df_guests
where gender_code = "male"`;

query q2
`select date_of_birth
from df_guests df_guests
where (gender_code = 'male')`;

verify q1 q2;