schema df_student(stuid:int, lname:str, fname:str, age:int, sex:str, major:int, advisor:int, city_code:str);

table df_student(df_student);

query q1
`select count(*)
from df_student df_student
where age = 18`;

query q2
`select n as count...
from
  (select count(*) as n
   from
     (select *
      from df_student df_student
      where (age = 18.0)))`;

verify q1 q2;