schema df_lots(lot_id:int, investor_id:int, lot_details:str);

table df_lots(df_lots);

query q1
`select distinct lot_details
from df_lots df_lots`;

query q2
`select lot_details
from
  (select lot_details
   from df_lots df_lots intersect select lot_details
   from df_lots df_lots)`;

verify q1 q2;