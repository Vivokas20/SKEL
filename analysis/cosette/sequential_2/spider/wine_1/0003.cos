schema df_wine(no:int, grape:str, winery:str, appelation:str, state:str, name:str, year:int, price:int, score:int, cases:str, drink:str);

table df_wine(df_wine);

query q1
`select name
from df_wine df_wine
where year = "2008"`;

query q2
`select name
from df_wine df_wine
where (year = 2008.0)`;

verify q1 q2;