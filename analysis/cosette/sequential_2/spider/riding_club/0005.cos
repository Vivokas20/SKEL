schema df_player(player_id:int, sponsor_name:str, player_name:str, gender:str, residence:str, occupation:str, votes:int, rank:str);

table df_player(df_player);

query q1
`select sponsor_name
from df_player df_player
where residence = "brandon"
  or residence = "birtle"`;

query q2
`select sponsor_name
from df_player df_player
where (residence = 'brandon'
       or residence = 'birtle')`;

verify q1 q2;