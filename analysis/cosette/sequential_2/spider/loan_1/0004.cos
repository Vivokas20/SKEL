schema df_bank(branch_id:int, bname:str, no_of_customers:int, city:str, state:str);

table df_bank(df_bank);

query q1
`select avg(no_of_customers)
from df_bank df_bank
where state = 'utah'`;

query q2
`select no_of_customers as avg.no_of_customers.
from df_bank df_bank
where (state = 'utah')`;

verify q1 q2;