schema df_farm_competition(competition_id:int, year:int, theme:str, host_city_id:int, hosts:str);

table df_farm_competition(df_farm_competition);

query q1
`select hosts
from df_farm_competition df_farm_competition
where theme != 'aliens'`;

query q2
`select hosts
from df_farm_competition df_farm_competition
where (theme != 'aliens')`;

verify q1 q2;