schema df_8_1(S_key:str, level:str, age:int);

table df_8_1(df_8_1);

query q1
`select level,
       meanage
from
  (select level,
          avg(age) as meanage
   from
     (select *
      from df_8_1 df_8_1
      where (level != 'jr'))
   group by level)`;

query q2
`select level,
       meanage
from
  (select level,
          avg(age) as meanage
   from
     (select *
      from input0
      where (level != 'jr'))
   group by level)`;

verify q1 q2;