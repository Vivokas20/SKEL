schema df_performers(performer_id:int, address_id:int, customer_name:str, customer_phone:str, customer_email_address:str, other_details:str);

table df_performers(df_performers);

query q1
`select customer_phone
from df_performers df_performers
where customer_name = "ashley"`;

query q2
`select distinct customer_phone
from input0
where (customer_name = 'ashley')`;

verify q1 q2;