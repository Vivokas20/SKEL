schema df_cinema(cinema_id:int, name:str, openning_year:int, capacity:int, location:str);

table df_cinema(df_cinema);

query q1
`select location
from df_cinema df_cinema
where capacity > 300
group by location
having count(*) > 1`;

query q2
`select distinct location
from
  (select openning_year,
          location,
          count(*) as n
   from input0
   group by openning_year,
            location)
where (n > 1.0
       or n > 300.0)`;

verify q1 q2;