schema df_flight(flno:int, origin:str, destination:str, distance:int, departure_date:str, arrival_date:str, price:str, aid:int);

table df_flight(df_flight);

query q1
`select flno
from df_flight df_flight
where distance > 2000`;

query q2
`select flno
from df_flight df_flight
where (distance >= 2000.0)`;

verify q1 q2;