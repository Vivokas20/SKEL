schema df_ship(ship_id:int, name:str, type:str, built_year:str, class:str, flag:str);

table df_ship(df_ship);

query q1
`select name
from df_ship df_ship

         class`;

query q2
`select name
from
  (select name
   from df_ship df_ship intersect select name
   from df_ship df_ship)`;

verify q1 q2;