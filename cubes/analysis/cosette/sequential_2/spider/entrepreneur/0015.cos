schema df_entrepreneur(entrepreneur_id:int, people_id:int, company:str, money_requested:str, investor:str);

table df_entrepreneur(df_entrepreneur);

query q1
`select investor
from df_entrepreneur df_entrepreneur
group by investor
having count(*) >= 2`;

query q2
`select investor
from
  (select investor,
          count(*) as n
   from df_entrepreneur df_entrepreneur
   group by investor)
where (n >= 2.0)`;

verify q1 q2;