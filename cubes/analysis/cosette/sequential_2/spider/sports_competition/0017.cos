schema df_competition(competition_id:int, year:str, competition_type:str, country:str);

table df_competition(df_competition);

query q1
`select competition_type
from df_competition df_competition
group by competition_type
having count(*) <= 5`;

query q2
`select competition_type
from
  (select competition_type,
          count(*) as n
   from df_competition df_competition
   group by competition_type)
where (n <= 5.0)`;

verify q1 q2;