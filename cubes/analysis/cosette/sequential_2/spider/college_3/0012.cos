schema df_department(dno:int, division:str, dname:str, room:str, building:str, dphone:int);

table df_department(df_department);

query q1
`select dname
from df_department df_department
where building = "mergenthaler"`;

query q2
`select dname
from df_department df_department
where (building = 'mergenthaler')`;

verify q1 q2;