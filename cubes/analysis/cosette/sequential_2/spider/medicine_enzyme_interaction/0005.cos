schema df_enzyme(id:int, name:str, location:str, product:str, chromosome:str, omim:int, porphyria:str);

table df_enzyme(df_enzyme);

query q1
`select product,
       chromosome,
       porphyria
from df_enzyme df_enzyme
where location = 'cytosol'`;

query q2
`select product,
       chromosome,
       porphyria
from df_enzyme df_enzyme
where (location = 'cytosol')`;

verify q1 q2;