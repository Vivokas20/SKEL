schema df_faculty(facid:int, lname:str, fname:str, rank:str, sex:str, phone:int, room:str, building:str);

table df_faculty(df_faculty);

query q1
`select fname,
       lname
from df_faculty df_faculty
where rank = "instructor"`;

query q2
`select fname,
       lname
from df_faculty df_faculty
where (rank = 'instructor')`;

verify q1 q2;