schema df_book_club(book_club_id:int, year:int, author_or_editor:str, book_title:str, publisher:str, category:str, result:str);

table df_book_club(df_book_club);

query q1
`select book_title,
       author_or_editor
from df_book_club df_book_club
where year > 1989`;

query q2
`select book_title,
       author_or_editor
from df_book_club df_book_club
where (year > 1989.0)`;

verify q1 q2;