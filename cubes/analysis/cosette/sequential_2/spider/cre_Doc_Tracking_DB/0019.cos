schema df_employees(employee_id:int, role_code:str, employee_name:str, gender_mfu:str, date_of_birth:str, other_details:str);

table df_employees(df_employees);

query q1
`select count(*)
from df_employees df_employees`;

query q2
`select n as count...
from
  (select count(*) as n
   from df_employees df_employees)`;

verify q1 q2;