schema df_club(clubid:int, clubname:str, clubdesc:str, clublocation:str);

table df_club(df_club);

query q1
`select clublocation
from df_club df_club
where clubname = "hopkins student enterprises"`;

query q2
`select input.clublocation
 from
  input
 where input.clubname = 'hopkins student enterprises'`;

verify q1 q2;